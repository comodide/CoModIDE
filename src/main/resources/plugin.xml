<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>

	<!-- Tabs -->
	
	<extension id="CoMoDIDETab" point="org.protege.editor.core.application.WorkspaceTab">
		<label value="CoMoDIDE"/>
		<class value="com.karlhammar.xdp.views.CoMoDIDETab"/>
		<index value="X"/>
		<editorKitId value="OWLEditorKit"/>
		<defaultViewConfigFileName value="viewconfig-CoMoDIDETab.xml"/>
   </extension>

	<!-- Views -->
	
	<extension id="CoMoDIDEView" point="org.protege.editor.core.application.ViewComponent">
		<label value="CoMoDIDE View" />
		<class value="com.karlhammar.xdp.views.CoMoDIDEView" />
		<headerColor value="@org.protege.ontologycolor"/>
	</extension>
    
</plugin>
