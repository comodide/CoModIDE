<mxStylesheet>
	<add as="defaultVertex">
		<add as="height" value="30" />
		<add as="width" value="120" />
		<add as="strokeColor" value="#000000" />
		<add as="strokeWidth" value="4" />
		<add as="fontSize" value="12" />
		<add as="fontColor" value="#000000" />
		<add as="fontFamily" value="Verdana" />
		<add as="spacing" value="5" />
		<add as="verticalLabelPosition" value="middle" />
		<add as="verticalAlign" value="middle" />
		<add as="autosize" value="1" />

	</add>

	<add as="comodideClassVertex" extend="defaultVertex">
		<add as="shape" value="rectangle" />
		<add as="perimeter" value="rectanglePerimeter" />
		<add as="rounded" value="1" />
		<add as="fillColor" value="#ffcc00" />
	</add>

	<add as="comodideDatatypeVertex" extend="defaultVertex">
		<add as="shape" value="ellipse" />
		<add as="perimeter" value="ellipsePerimeter" />
		<add as="fillColor" value="#b0e0e0" />
	</add>

	<add as="defaultEdge">
		<add as="labelBackgroundColor" value="white" />
		<add as="rounded" value="1" />
		<add as="edgeStyle" value="elbowEdgeStyle" />
		<add as="strokeColor" value="#000000" />
		<add as="fontSize" value="12" />
		<add as="fontColor" value="#000000" />
		<add as="fontFamily" value="Verdana" />
		<add as="shape" value="connector" />
		<add as="endArrow" value="block" />
		<add as="strokeWidth" value="2" />
	</add>

	<add as="comodideStandardEdge" extend="defaultEdge">
		<add as="endSize" value="15" />
	</add>

	<add as="comodideSubclassEdge" extend="defaultEdge">
		<add as="endSize" value="20" />
		<add as="dashed" value="1" />
		<add as="endFill" value="0" />
	</add>

	<add as="moduleVertex">
		<add as="verticalLabelPosition" value="top" />
		<add as="labelPosition" value="right" />
		<add as="strokeColor" value="#000000" />
		<add as="strokeWidth" value="4" />
		<add as="dashed" value="1" />
		<add as="autosize" value="1" />
		<add as="fillColor" value="#0f0f0f" />
	</add>

</mxStylesheet>
