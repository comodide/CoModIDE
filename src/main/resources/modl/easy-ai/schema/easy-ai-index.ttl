opla:categorization rdfs:domain opla:Pattern ;
    rdfs:range opla:Category .

opla:htmlDocumentation a owl:DatatypeProperty ;
    rdfs:label "Owl HTML Documentation"@en ;
    rdfs:domain opla:Pattern ;
    rdfs:range xsd:string .

opla:owlRepresentation a owl:DatatypeProperty ;
    rdfs:label "Owl Representation"@en ;
    rdfs:domain opla:Pattern ;
    rdfs:range xsd:string .

opla:renderedSchemaDiagram a owl:DatatypeProperty ;
    rdfs:label "Rendered Schema Diagram"@en ;
    rdfs:domain opla:Pattern ;
    rdfs:range xsd:string .

: a owl:Ontology ;
