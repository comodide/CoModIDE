      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Temporal_region"/>
  </owl:Class>

  <owl:Class rdf:about="#Token">
    <rdfs:subClassOf rdf:resource="#Concrete"/>
